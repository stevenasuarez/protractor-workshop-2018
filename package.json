{
  "name": "workshop-protractor",
  "version": "1.0.0",
  "description": "This is a workshop about Protractor",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "test:local": "npm run build && protractor dist/protractor/config.js",
    "test:headless": "npm run build && protractor dist/protractor/headless.conf.js",
    "test": "npm run test:headless",
    "postinstall": "webdriver-manager update --gecko false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevenasuarez/protractor-workshop-2018.git"
  },
  "keywords": [
    "ui-testing",
    "dojo",
    "practice",
    "protactor"
  ],
  "author": "steven suarez ssuarezc@psl.com.co",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stevenasuarez/protractor-workshop-2018/issues"
  },
  "homepage": "https://github.com/stevenasuarez/protractor-workshop-2018#readme",
  "dependencies": {
    "jasmine-spec-reporter": "^4.2.1",
    "protractor": "^5.3.1"
  },
  "devDependencies": {
    "@types/jasminewd2": "^2.0.3",
    "typescript": "^2.8.3"
  }
}
